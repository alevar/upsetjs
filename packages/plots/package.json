{
  "name": "@upsetjs/plots",
  "description": "UpSet.js is a re-implementation of UpSetR to create interactive set visualizations for more than three sets",
  "version": "1.7.0",
  "license": "SEE LICENSE IN LICENSE.md",
  "author": {
    "name": "Samuel Gratzl",
    "email": "sam@sgratzl.com",
    "url": "https://wwww.sgratzl.com"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "homepage": "https://github.com/upsetjs/upsetjs",
  "bugs": {
    "url": "https://github.com/upsetjs/upsetjs/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/upsetjs/upsetjs.git"
  },
  "module": "dist/plots.esm.js",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "sideEffects": false,
  "scripts": {
    "clean": "rimraf dist docs \"*.tgz\"",
    "compile": "tsc -p tsconfig.json --noEmit",
    "start": "npm run watch",
    "watch": "rollup -c -w",
    "build": "rollup -c",
    "test": "jest --passWithNoTests",
    "test:watch": "jest --passWithNoTests --watch",
    "test:coverage": "jest --passWithNoTests --coverage",
    "lint": "npm run eslint && npm run prettier",
    "fix": "npm run eslint:fix && npm run prettier:write",
    "prettier:write": "prettier */** --write",
    "prettier": "prettier */** --check",
    "eslint": "eslint src --ext .ts,.tsx",
    "eslint:fix": "npm run eslint -- --fix",
    "prepare": "npm run build",
    "storybook": "start-storybook -p 8080",
    "docs": "build-storybook --quiet -o ./docs"
  },
  "peerDependencies": {
    "react": ">=17"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@rollup/plugin-commonjs": "^17.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^11.0.0",
    "@rollup/plugin-replace": "^2.3.4",
    "@rollup/plugin-typescript": "^8.0.0",
    "@storybook/addon-docs": "^6.1.9",
    "@storybook/addons": "^6.1.9",
    "@storybook/preset-typescript": "^3.0.0",
    "@storybook/react": "^6.1.9",
    "@testing-library/react": "^11.2.2",
    "@types/jest": "^26.0.16",
    "@types/react-dom": "^17.0.0",
    "@types/testing-library__react": "^10.0.1",
    "@typescript-eslint/eslint-plugin": "^4.9.0",
    "@typescript-eslint/parser": "^4.9.0",
    "babel-loader": "^8.2.2",
    "core-js": "^3.8.0",
    "eslint": "^7.14.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-config-react-app": "^6.0.0",
    "eslint-plugin-flowtype": "^5.2.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-testing-library": "^3.10.1",
    "jest": "^26.6.3",
    "prettier": "^2.2.1",
    "react": "^16.13.0",
    "react-docgen-typescript-loader": "^3.7.2",
    "react-dom": "^17.0.1",
    "react-is": "^17.0.1",
    "rimraf": "^3.0.2",
    "rollup": "^2.34.0",
    "rollup-plugin-dts": "^2.0.0",
    "ts-jest": "^26.4.4",
    "ts-loader": "^8.0.11",
    "tslib": "^2.0.3",
    "typescript": "^4.1.2",
    "webpack": "^5.9.0"
  },
  "dependencies": {
    "@types/lodash.throttle": "^4.1.6",
    "@types/react": "^17.0.0",
    "@upsetjs/react": "~1.7.0",
    "lodash.throttle": "^4.1.1",
    "react-vega": "^7.4.2",
    "vega": "^5.17.0",
    "vega-lite": "^4.17.0",
    "vega-typings": "^0.19.1"
  }
}
