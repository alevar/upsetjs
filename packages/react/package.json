{
  "name": "@upsetjs/react",
  "description": "UpSet.js is a re-implementation of UpSetR to create interactive set visualizations for more than three sets",
  "version": "0.5.0",
  "license": "SEE LICENSE IN LICENSE.md",
  "author": {
    "name": "Samuel Gratzl",
    "email": "sam@sgratzl.com",
    "url": "https://wwww.sgratzl.com"
  },
  "publishConfig": {
    "access": "public"
  },
  "homepage": "https://github.com/upsetjs/upsetjs",
  "bugs": {
    "url": "https://github.com/upsetjs/upsetjs/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/sgratzl/upsetjs.git"
  },
  "module": "dist/react.esm.js",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "scripts": {
    "compile": "tsc -p tsconfig.json --noEmit",
    "start": "tsdx watch --noClean",
    "build": "tsdx build",
    "test": "tsdx test --passWithNoTests",
    "test:watch": "tsdx test --passWithNoTests --watch",
    "test:coverage": "tsdx test --passWithNoTests --coverage",
    "lint": "npm run eslint && npm run prettier:check",
    "fix": "npm run eslint:fix && npm run prettier:write",
    "prettier": "prettier \"*.{md,json,js}\" \"{src,.storybook,sample}/**\"",
    "prettier:write": "npm run prettier -- --write",
    "prettier:check": "npm run prettier -- --check",
    "eslint": "tsdx lint src --ext .ts,.tsx",
    "eslint:fix": "npm run eslint -- --fix",
    "prepare": "tsdx build",
    "storybook": "start-storybook -p 6006",
    "docs": "build-storybook --quiet --no-dll -o ./docs"
  },
  "browserslist": [
    "defaults"
  ],
  "peerDependencies": {
    "react": ">=16"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@rollup/plugin-commonjs": "^11.0.2",
    "@storybook/addon-docs": "^6.0.0-alpha.20",
    "@storybook/addons": "^6.0.0-alpha.20",
    "@storybook/client-api": "^6.0.0-alpha.20",
    "@storybook/client-logger": "^5.3.18",
    "@storybook/react": "^6.0.0-alpha.20",
    "@types/jest": "^25.1.4",
    "@types/react-dom": "^16.9.6",
    "@typescript-eslint/eslint-plugin": "^2.26.0",
    "@typescript-eslint/parser": "^2.26.0",
    "@yarnpkg/pnpify": "^2.0.0-rc.20",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-config-react-app": "^5.2.1",
    "eslint-plugin-flowtype": "^4.7.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^3.0.0",
    "pnp-webpack-plugin": "^1.6.4",
    "prettier": "^2.0.2",
    "react": "^16.13.0",
    "react-docgen-typescript-loader": "^3.7.2",
    "react-dom": "^16.13.1",
    "react-is": "^16.13.1",
    "rollup-plugin-pnp-resolve": "^2.0.0",
    "ts-loader": "^6.2.2",
    "tsdx": "~0.12.0",
    "tslib": "^1.11.1",
    "typescript": "^3.8.3"
  },
  "dependencies": {
    "@types/react": "^16.9.31",
    "@upsetjs/model": "^0.5.0"
  }
}
