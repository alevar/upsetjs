    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.25.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.3.2",
    "pnp-webpack-plugin": "^1.7.0",
    "prettier": "^2.3.2",
    "rimraf": "^3.0.2",
    "style-loader": "^3.2.1",
    "ts-loader": "^9.2.5",
    "tslib": "^2.3.1",
