  "scripts": {
    "lint": "npm run eslint && npm run prettier:check",
    "fix": "npm run eslint:fix && npm run prettier:write",
    "prettier": "prettier \"*.{md,json,js}\" \"src/**\"",
    "prettier:write": "npm run prettier -- --write",
    "prettier:check": "npm run prettier -- --check",
    "eslint": "eslint src --ext .ts,.tsx",
    "eslint:fix": "npm run eslint -- --fix",
    "clean": "rimraf dist",
    "build": "npm run clean && webpack-cli -p",
    "build:stats": "webpack-cli -p --json > dist/stats.json",
    "build:dev": "webpack-cli -d --devtool none",
    "start": "webpack-dev-server --devtool cheap-module-eval-source-map"
  },
  "browserslist": [
    "last 2 Chrome versions",
    "last 2 Firefox versions",
    "Firefox ESR"
  ],
