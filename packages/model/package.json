{
  "name": "@upsetjs/model",
  "description": "UpSet.js is a re-implementation of UpSetR to create interactive set visualizations for more than three sets",
  "version": "1.0.0",
  "license": "SEE LICENSE IN LICENSE.md",
  "author": {
    "name": "Samuel Gratzl",
    "email": "sam@sgratzl.com",
    "url": "https://wwww.sgratzl.com"
  },
  "publishConfig": {
    "access": "public"
  },
  "homepage": "https://github.com/upsetjs/upsetjs",
  "bugs": {
    "url": "https://github.com/upsetjs/upsetjs/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/sgratzl/upsetjs.git"
  },
  "module": "dist/model.esm.js",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "sideEffects": false,
  "scripts": {
    "compile": "tsc -p tsconfig.json --noEmit",
    "start": "tsdx watch --noClean",
    "clean": "rimraf .tmp dist",
    "build": "npm run clean && npm run build:impl && npm run typings",
    "build:impl": "tsdx build",
    "typings": "tsc -p tsconfig.dts.json && rollup -c rollup.dts.config.js",
    "test": "tsdx test --passWithNoTests",
    "test:watch": "tsdx test --passWithNoTests --watch",
    "test:coverage": "tsdx test --passWithNoTests --coverage",
    "lint": "npm run eslint && npm run prettier",
    "fix": "npm run eslint:fix && npm run prettier:write",
    "prettier:impl": "prettier \"*.{md,json,js}\" \"src/**\"",
    "prettier:write": "npm run prettier:impl -- --write",
    "prettier": "npm run prettier:impl -- --check",
    "eslint": "tsdx lint src --ext .ts,.tsx",
    "eslint:fix": "npm run eslint -- --fix",
    "prepare": "tsdx build",
    "docs": "typedoc"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^11.1.0",
    "@types/jest": "^25.2.1",
    "@typescript-eslint/eslint-plugin": "^2.30.0",
    "@typescript-eslint/parser": "^2.30.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-config-react-app": "^5.2.1",
    "eslint-plugin-flowtype": "^4.7.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^4.0.0",
    "prettier": "^2.0.5",
    "rimraf": "^3.0.2",
    "rollup": "^2.10.7",
    "rollup-plugin-cleanup": "^3.1.1",
    "rollup-plugin-dts": "^1.4.7",
    "rollup-plugin-pnp-resolve": "^2.0.0",
    "tsdx": "~0.13.2",
    "tslib": "^1.11.1",
    "typedoc": "^0.17.6",
    "typescript": "^3.8.3"
  }
}
