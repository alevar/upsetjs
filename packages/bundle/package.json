{
  "name": "@upsetjs/bundle",
  "description": "UpSet.js is a re-implementation of UpSetR to create interactive set visualizations for more than three sets",
  "version": "0.6.0",
  "license": "SEE LICENSE IN LICENSE.md",
  "author": {
    "name": "Samuel Gratzl",
    "email": "sam@sgratzl.com",
    "url": "https://wwww.sgratzl.com"
  },
  "publishConfig": {
    "access": "public"
  },
  "homepage": "https://github.com/upsetjs/upsetjs",
  "bugs": {
    "url": "https://github.com/upsetjs/upsetjs/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/sgratzl/upsetjs.git"
  },
  "module": "dist/upsetjs.esm.js",
  "main": "dist/upsetjs.umd.development.js",
  "unpkg": "dist/upsetjs.umd.production.min.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "sideEffects": false,
  "scripts": {
    "start": "tsdx watch  --noClean --name UpSetJS --format esm,umd",
    "build": "tsdx build --name UpSetJS --format esm,umd",
    "test": "tsdx test --passWithNoTests",
    "test:watch": "tsdx test --passWithNoTests --watch",
    "test:coverage": "tsdx test --passWithNoTests --coverage",
    "lint": "npm run eslint && npm run prettier:check",
    "fix": "npm run eslint:fix && npm run prettier:write",
    "prettier": "prettier \"*.{md,json,js}\" \"{src,example}/**\"",
    "prettier:write": "npm run prettier -- --write",
    "prettier:check": "npm run prettier -- --check",
    "eslint": "tsdx lint src --ext .ts,.tsx",
    "eslint:fix": "npm run eslint -- --fix",
    "prepare": "tsdx build",
    "docs": "typedoc"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@rollup/plugin-alias": "^3.0.1",
    "@rollup/plugin-commonjs": "^11.0.2",
    "@types/jest": "^25.1.4",
    "@typescript-eslint/eslint-plugin": "^2.26.0",
    "@typescript-eslint/parser": "^2.26.0",
    "@upsetjs/addons": "^0.6.0",
    "@upsetjs/react": "^0.6.0",
    "babel-eslint": "^10.1.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-config-react-app": "^5.2.1",
    "eslint-plugin-flowtype": "^4.7.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^3.0.0",
    "preact": "^10.3.4",
    "prettier": "^2.0.2",
    "rollup-plugin-pnp-resolve": "^2.0.0",
    "tsdx": "~0.12.0",
    "tslib": "^1.11.1",
    "typedoc": "next",
    "typescript": "^3.8.3"
  },
  "dependencies": {
    "@upsetjs/math": "~0.6.0",
    "@upsetjs/model": "~0.6.0"
  }
}
